{
    "manifest_version": 2,
    "name": "YurHistory",
    "description": "Просмотр и редактирование истории поиска",
    "version": "1.0.0",
    "icons": {
        "128": "img/icon.png"
    },
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "history"
    ]
}